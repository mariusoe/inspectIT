		<classes>
			<class
				name="info.novatec.inspectit.storage.processor.StorageDataProcessorsTest" />
		</classes>
	</test>
	<test name="Serializer">
		<classes>
			<class name="info.novatec.inspectit.storage.serializer.impl.SerializerTest" />
			<class name="info.novatec.inspectit.storage.serializer.impl.BackwardForwardCompatibilityTest" />
		</classes>
	</test>
	<test name="Schema Test">
		<classes>
			<class
				name="info.novatec.inspectit.storage.serializer.schema.SchemaTest" />
		</classes>
	</test>
	<test name="Storage">
		<classes>
			<class name="info.novatec.inspectit.storage.StorageIntegrationTest" />
			<class name="info.novatec.inspectit.storage.StorageWriterTest" />
		</classes>
	</test>
	<test name="EqualsVerifier">
		<classes>
			<class name="info.novatec.inspectit.communication.EqualsVerifierTest" />
		</classes>
	</test>
	<test name="Memory Calculation Test">
		<classes>
			<class
				name="info.novatec.inspectit.cmr.cache.impl.MemoryCalculationTest" />
		</classes>
	</test>
