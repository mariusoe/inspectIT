<!DOCTYPE suite SYSTEM "http://beust.com/testng/testng-1.0.dtd" >
<suite name="CMRTest">
	<test name="Cache">
		<classes>
			<class name="info.novatec.inspectit.cmr.cache.impl.BufferPropertiesTest" />
			<class name="info.novatec.inspectit.cmr.cache.impl.ObjectSizesPrimitiveTypesSizeTest" />
		</classes>
	</test>
	<test name="IndexingRestriction">
		<classes>
			<class name="info.novatec.inspectit.indexing.restriction.IndexQueryRestrictionProcessorTest" />
		</classes>
	</test>
	<test name="Indexing">
		<classes>
			<class name="info.novatec.inspectit.indexing.impl.BufferIndexingTest" />
			<class name="info.novatec.inspectit.indexing.impl.StorageIndexingTest" />
		</classes>
	</test>
	<test name="Dao/Aggregators">
		<classes>
			<class name="info.novatec.inspectit.indexing.aggregation.impl.HttpDataAggregatorTest" />
			<class name="info.novatec.inspectit.cmr.dao.impl.TimerDataAggregatorTest" />
			<class name="info.novatec.inspectit.cmr.dao.impl.PlatformIdentDaoTest" />
		</classes>
	</test>
	<test name="Service">
		<classes>
			<class name="info.novatec.inspectit.cmr.service.RegistrationServiceTest" />
			<class name="info.novatec.inspectit.cmr.service.AgentStorageServiceTest" />
			<class name="info.novatec.inspectit.cmr.service.GlobalDataAccessServiceTest" />
		</classes>
	</test>
	<test name="Util">
		<classes>
			<class name="info.novatec.inspectit.cmr.util.PlatformIdentCacheTest" />
		</classes>
	</test>
	<test name="Data">
		<classes>
			<class name="info.novatec.inspectit.communication.data.InvocationAwareDataTest" />
			<class name="info.novatec.inspectit.communication.data.AggregatedDataTest" />
		</classes>
	</test>
	<test name="NIO">
		<classes>
			<class name="info.novatec.inspectit.storage.nio.ChannelManagersTest" />
			<class name="info.novatec.inspectit.storage.nio.ByteBufferProviderTest" />
			<class name="info.novatec.inspectit.storage.nio.stream.ExtendedByteBufferOutputStreamTest" />
			<class name="info.novatec.inspectit.storage.nio.CompletionHandlersTest" />
