<ivysettings>
	<settings defaultResolver="external-repository"/>

	<resolvers>	
	<chain name="external-repository">
		<ibiblio name="javanet" root="http://download.java.net/maven/2/" m2compatible="true" usepoms="true"/>
		<ibiblio name="jboss" root="http://repository.jboss.com/maven2/" m2compatible="true" usepoms="true"/>
		<ibiblio name="jboss-public" root="https://repository.jboss.org/nexus/content/groups/public/" m2compatible="true" usepoms="true"/>
		<ibiblio name="jboss-public2" root="https://repository.jboss.org/nexus/content/groups/public-jboss/" m2compatible="true" usepoms="true"/>
		<ibiblio name="eviware" root="http://www.eviware.com/repository/maven2/" m2compatible="true" usepoms="true"/>
		<ibiblio name="maven" root="http://repo1.maven.org/maven2" m2compatible="true" usepoms="true"/>
	</chain>
	</resolvers>
</ivysettings>

