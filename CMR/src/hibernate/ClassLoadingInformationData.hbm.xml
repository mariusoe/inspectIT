<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="info.novatec.novaspy.communication.data">
	<union-subclass name="ClassLoadingInformationData"
		extends="info.novatec.novaspy.communication.SystemSensorData"
		table="classloading_information_data">
		<property name="count" column="count" />
		<property name="minLoadedClassCount"
			column="min_loaded_class_count" />
		<property name="maxLoadedClassCount"
			column="max_loaded_class_count" />
		<property name="totalLoadedClassCount"
			column="total_loaded_class_count" />
		<property name="minTotalLoadedClassCount"
			column="min_total_loaded_class_count" />
		<property name="maxTotalLoadedClassCount"
			column="max_total_loaded_class_count" />
		<property name="totalTotalLoadedClassCount"
			column="total_total_loaded_class_count" />
		<property name="minUnloadedClassCount"
			column="min_unloaded_class_count" />
		<property name="maxUnloadedClassCount"
			column="max_unloaded_class_count" />
		<property name="totalUnloadedClassCount"
			column="total_unloaded_class_count" />
	</union-subclass>
</hibernate-mapping>