<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="info.novatec.inspectit.communication.data">
	<class name="ParameterContentData" table="parameter_content_data">
		<id name="id" type="java.lang.Long" column="id">
			<generator class="seqhilo" />
		</id>
		<property name="methodSensorId" column="method_sensor_id" />
		<property name="name" column="name" />
		<property name="content" column="content" length="10000" />
		<property name="methodParameter" column="isMethodParameter" />
		<property name="signaturePosition" column="signaturePosition" />
	</class>
</hibernate-mapping>