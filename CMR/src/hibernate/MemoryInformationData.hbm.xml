<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="info.novatec.inspectit.communication.data">
	<union-subclass name="MemoryInformationData"
		extends="info.novatec.inspectit.communication.SystemSensorData"
		table="memory_information_data">
		<property name="count" 
			column="count" />
		<property name="minFreePhysMemory"
			column="min_free_phys_memory" />
		<property name="maxFreePhysMemory"
			column="max_free_phys_memory" />
		<property name="totalFreePhysMemory"
			column="total_free_phys_memory" />
		<property name="minFreeSwapSpace" 
			column="min_free_swap_space" />
		<property name="maxFreeSwapSpace" 
			column="max_free_swap_space" />
		<property name="totalFreeSwapSpace"
			column="total_free_swap_space" />
		<property name="minComittedVirtualMemSize"
			column="min_comitted_virtual_mem_size" />
		<property name="maxComittedVirtualMemSize"
			column="max_comitted_virtual_mem_size" />
		<property name="totalComittedVirtualMemSize"
			column="total_comitted_virtual_mem_size" />
		<property name="minUsedHeapMemorySize"
			column="min_used_heap_memory_size" />
		<property name="maxUsedHeapMemorySize"
			column="max_used_heap_memory_size" />
		<property name="totalUsedHeapMemorySize"
			column="total_used_heap_memory_size" />
		<property name="minUsedNonHeapMemorySize"
			column="min_used_non_heap_memory_size" />
		<property name="maxUsedNonHeapMemorySize"
			column="max_used_non_heap_memory_size" />
		<property name="totalUsedNonHeapMemorySize"
			column="total_used_non_heap_memory_size" />
		<property name="minComittedHeapMemorySize"
			column="min_comitted_heap_memory_size" />
		<property name="maxComittedHeapMemorySize"
			column="max_comitted_heap_memory_size" />
		<property name="totalComittedHeapMemorySize"
			column="total_comitted_heap_memory_size" />
		<property name="minComittedNonHeapMemorySize"
			column="min_comitted_non_heap_memory_size" />
		<property name="maxComittedNonHeapMemorySize"
			column="max_comitted_non_heap_memory_size" />
		<property name="totalComittedNonHeapMemorySize"
			column="total_comitted_non_heap_memory_size" />
	</union-subclass>
</hibernate-mapping>