<?xml version="1.0" encoding="ISO-8859-1"?>
<ivy-module version="2.0" xmlns:e="http://ant.apache.org/ivy/extra">
    <info organisation="info.novatec.inspectit" module="Agent"/>
    
    <!-- http://www.jarvana.com/jarvana/ is a nice page for finding projects-->
    
    <configurations>
    	<conf name="prod" visibility="private" transitive="false"/>
    	<conf name="test" visibility="private" extends="prod" transitive="false"/>
    </configurations>
    
	<dependencies> 
		<dependency org="javassist" name="javassist" rev="3.10.0.GA" conf="prod->default" transitive="false"/>
		<dependency org="org.picocontainer" name="picocontainer" rev="1.3" conf="prod->default" transitive="false"/>
		
		
		<dependency org="org.testng" name="testng" rev="5.8" transitive="false" conf="test->default">
        	<artifact name="testng" type="jar" ext="jar" e:classifier="jdk15" />
      	</dependency>
		<dependency org="org.mockito" name="mockito-all" rev="1.7" conf="test->default"/>
		<dependency org="net.sourceforge.cobertura" name="cobertura" rev="1.9.3" conf="test->default"/>
		<dependency org="log4j" name="log4j" rev="1.2.9" conf="test->default"/>
		<dependency org="apache-oro" name="jakarta-oro" rev="2.0.8" conf="test->default"/>
		<dependency org="asm" name="asm" rev="3.0" conf="test->default"/>
		<dependency org="asm" name="asm-tree" rev="3.0" conf="test->default"/>
		<dependency org="asm" name="asm-all" rev="3.0" conf="test->default"/>
		
		
		<dependency org="pmd" name="pmd" rev="4.2.5" conf="test->default"/>
		<!-- necessary for pmd -->
		<dependency org="jaxen" name="jaxen" rev="1.1.1" conf="test->default"/>
		
		
		<dependency org="checkstyle" name="checkstyle" rev="5.0" conf="test->default"/>
		<dependency org="antlr" name="antlr" rev="2.7.6" conf="test->default"/>
		<dependency org="commons-beanutils" name="commons-beanutils" rev="1.7.0" conf="test->default"/>
		<dependency org="commons-cli" name="commons-cli" rev="1.0" conf="test->default"/>
		<dependency org="commons-logging" name="commons-logging" rev="1.0.3" conf="test->default"/>
		<dependency org="commons-lang" name="commons-lang" rev="2.4" conf="test->default"/>
		<dependency org="com.google.collections" name="google-collections" rev="0.9" conf="test->default"/>
		
		<dependency org="com.google.code.findbugs" name="findbugs" rev="1.3.9" conf="test->default"/>
		<dependency org="com.google.code.findbugs" name="findbugs-ant" rev="1.3.9" conf="test->default"/>
		<dependency org="com.google.code.findbugs" name="jFormatString" rev="1.3.9" conf="test->default"/>
		<dependency org="com.google.code.findbugs" name="jsr305" rev="1.3.9" conf="test->default"/>
		<dependency org="com.google.code.findbugs" name="bcel" rev="1.3.9" conf="test->default"/>
		<dependency org="dom4j" name="dom4j" rev="1.6.1" conf="test->default"/>
	</dependencies>
</ivy-module>
